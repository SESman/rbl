year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{rbl}: Biologging tools for diving predators",
         author = person("Yves", "Le Bras"),
         year = year,
         note = note,
         url = "https://github.com/SESman/rbl",
         textVersion = paste0("Yves Le Bras (", year, "). rbl: Biologging tools for diving predators. ", note, ". URL https://github.com/SESman/rbl."))
